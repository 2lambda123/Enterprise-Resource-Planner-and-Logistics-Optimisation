    <string name="slide_1_title">Welcome to the Go-Fleet Navigation App</string>
    <string name="slide_1_desc">The easiest way to navigate and report on orders from your smartphone!</string>
    <string name="slide_2_title">Start by navigating to orders</string>
    <string name="slide_2_desc">Here you will find your daily orders!</string>
    <string name="slide_3_title">Commence orders in a Sequential order</string>
    <string name="slide_3_desc">Start with 1 then 2 ...</string>
    <string name="slide_4_title">Order details</string>
    <string name="slide_4_desc">Easily navigate to start by clicking green truck button</string>
    <string name="slide_5_title">GPS</string>
    <string name="slide_5_desc">Follow GPS to destination and delay this order or route if needed</string>
    <string name="slide_6_title">Order details</string>
    <string name="slide_6_desc">Click Camera button to scan client invoice for confirmation of order</string>

    <string name="slide_7_title">Order Confirmation</string>
    <string name="slide_7_desc">Scan client QR code from client invoice</string>

    <string name="slide_8_title">Delay order report page</string>
    <string name="slide_8_desc">Select appropriate delay type</string>

    <string name="slide_9_title">Press drop down and select reason for delay</string>
    <string name="slide_9_desc">Click reason for delay</string>

    <string name="slide_10_title">Select how long time delay will be for Temporary route delay</string>
    <string name="slide_10_desc">This will report a temporary delay to Phaki and Clients effected</string>

    <string name="slide_11_title">To delay a specific order select the order to be delayed/postponed</string>
    <string name="slide_11_desc">Press button to select order</string>

    <string name="slide_12_title">Press order you wish to postpone/delay</string>
    <string name="slide_12_desc">Do not select orders that have already been completed</string>

    <string name="slide_13_title">To finalize report click the delay button</string>
    <string name="slide_13_desc">Remember to always enter reason for delay</string>

    <string name="slide_14_title">Login Screen</string>
    <string name="slide_14_desc">Enter your assigned driver details (Username and Password) and click login</string>

    <string name="slide_15_title">Driver Menu</string>
    <string name="slide_15_desc">Here you can navigate to appropriate driver functions</string>

    <string name="slide_16_title">Second Home Screen</string>
    <string name="slide_16_desc">Select appopriate functionality when needed</string>

    <string name="slide_17_title">Welcome to GoFleet Navigation</string>
    <string name="slide_17_desc">Safe travels fellow driver!</string>

    <string name="slide_18_title">Overview of route</string>
    <string name="slide_18_desc">Select Green to start route or if returning select orange</string>

